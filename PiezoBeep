#include "simpletools.h"                      // Include simpletools                   

int main()                                    // main function             
{
  //freqout(4, 1000, 3000);                     // pin, duration, frequency
  freqout(4, 400, 1568);      //G6 1568
  freqout(4, 400, 1568);
  freqout(4, 500, 1760);      //A6 1760
  
  freqout(4, 500, 1568);
  freqout(4, 500, 2093);      //C7 2093
  freqout(4, 1000, 1976);    //B6 1975.5
  
  freqout(4, 400, 1568);      //G6 1568
  freqout(4, 400, 1568);
  freqout(4, 500, 1760);      //A6 1760
  
  freqout(4, 500, 1568);      //G6 1568
  freqout(4, 500, 2349);      //D7 2349
  freqout(4, 1000, 2093);      //C7 2093
}
